      "source": [
        "def fibb (x):\n",
        "    if x < 4:\n",
        "        return x\n",
        "    else:\n",
        "        return fibb(x-1) + fibb(x -2)\n",
        "\n",
        "fibb(14)\n",
        "\n",
        "for y in range(14):\n",
        "    print(fibb(y))"
      ],
