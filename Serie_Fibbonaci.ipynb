        "def fibb(start):\n",
        "    if start == 0:\n",
        "        return 0\n",
        "    elif start == 1:\n",
        "        return 1\n",
        "    return fibb(start -1)  +  fibb(start -2)\n",
        "\n",
        "print(\"serie finobbaci\".center(50,'-'))\n",
        "\n",
        "print(f\"{fibb(18)}\")"
